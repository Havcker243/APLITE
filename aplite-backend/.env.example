DATABASE_URL=postgresql://user:password@localhost:5432/aplite
ENCRYPTION_KEY=change_me_32_bytes_minimum
UPI_SECRET_KEY=change_me
SESSION_TTL_HOURS=168
ADMIN_API_KEY=change_me
CSRF_SECRET_KEY=change_me

# Global rate limit (0 = disabled)
RL_GLOBAL_LIMIT=300
RL_GLOBAL_WINDOW_SECONDS=60

# Resolve + lookup
RL_RESOLVE_LIMIT=60
RL_RESOLVE_WINDOW_SECONDS=60
RL_UPI_LOOKUP_LIMIT=120
RL_UPI_LOOKUP_WINDOW_SECONDS=60
RL_UPI_MASTER_LOOKUP_LIMIT=60
RL_UPI_MASTER_LOOKUP_WINDOW_SECONDS=60

# Onboarding
RL_ONBOARDING_UPLOAD_ID_LIMIT=20
RL_ONBOARDING_UPLOAD_ID_WINDOW_SECONDS=3600
RL_ONBOARDING_UPLOAD_FORMATION_LIMIT=20
RL_ONBOARDING_UPLOAD_FORMATION_WINDOW_SECONDS=3600
RL_ONBOARDING_COMPLETE_LIMIT=5
RL_ONBOARDING_COMPLETE_WINDOW_SECONDS=3600

# Accounts + UPIs
RL_ACCOUNTS_CREATE_LIMIT=20
RL_ACCOUNTS_CREATE_WINDOW_SECONDS=3600
RL_CHILD_UPI_CREATE_LIMIT=30
RL_CHILD_UPI_CREATE_WINDOW_SECONDS=3600

# Public clients
RL_PUBLIC_CLIENTS_LIMIT=120
RL_PUBLIC_CLIENTS_WINDOW_SECONDS=60
